<script type="text/javascript">
	$(function(){

		$("#cboWidgetUsuarios").attr("disabled","true");

		$("#widgetChkHabilitaUsuarios").click(function(e){
			if(this.checked){
				$("#cboWidgetUsuarios").removeAttr("disabled");
			}else{
				$("#cboWidgetUsuarios").attr("disabled","true");				
			}
		});

	});
</script>

